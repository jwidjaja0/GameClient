<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>

<HBox id="menuHbox" prefHeight="24.0" prefWidth="508.0" stylesheets="@../CSS/TTTall.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ExceptionHandled.TicTacToeUI.MenuLayout.MenuLayoutController">
   <children>
      <MenuBar fx:id="menuBar1">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem fx:id="loginRegisterButton" mnemonicParsing="false" text="Login\Register" />
                  <MenuItem fx:id="logout" mnemonicParsing="false" text="Logout" />
              <MenuItem fx:id="closeOption" mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Profile">
               <items>
                  <MenuItem fx:id="personalRecordButton" mnemonicParsing="false" text="Personal Record" />
                  <MenuItem fx:id="changeProfileInfo" mnemonicParsing="false" text="Change Profile Information" />
                  <MenuItem fx:id="deleteAccount" mnemonicParsing="false" text="Delete Account" />
               </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</HBox>
